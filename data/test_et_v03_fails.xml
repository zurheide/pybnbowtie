<?xml version="1.0"?>
<opsa-mef>
  <define-fault-tree name="Simple Fault Tree">
    <define-gate name="OR">
      <label>Test of an OR gate</label>
      <or>
        <basic-event name="X1"/>
        <basic-event name="X2"/>
      </or>
    </define-gate>
  </define-fault-tree>
  <define-initiating-event name="OR" event-tree="EventTree"/>
  <define-event-tree name="EventTree">
    <define-functional-event name="e1"/>
    <define-sequence name="C1"/>
    <define-sequence name="C2"/>
    <initial-state>
      <fork functional-event="e1">
        <path state="fails">
          <collect-expression>
            <float value="0.9"/>
          </collect-expression>
          <sequence name="C2"/>
        </path>
        <path state="works">
          <collect-expression>
            <float value="0.1"/>
          </collect-expression>
          <sequence name="C1"/>
        </path>
      </fork>
    </initial-state>
  </define-event-tree>
  <model-data>
    <define-basic-event name="X1">
      <label>First basic event</label>
      <float value="1.0"/>
    </define-basic-event>
    <define-basic-event name="X2">
      <label>Second basic event</label>
      <float value="1.0"/>
    </define-basic-event>
  </model-data>
</opsa-mef>
